<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:ignore="MissingConstraints">


        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="하셨어요"
            android:textColor="@color/black"
            android:textSize="20dp"
            tools:layout_editor_absoluteX="52dp"
            tools:layout_editor_absoluteY="262dp" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="현재"
            android:textColor="@color/black"
            android:textSize="20dp"
            tools:layout_editor_absoluteX="52dp"
            tools:layout_editor_absoluteY="139dp" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="퇴근"
            android:textColor="@color/black"
            android:textSize="72dp"
            android:textStyle="bold"
            tools:layout_editor_absoluteX="52dp"
            tools:layout_editor_absoluteY="166dp" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/button123"
            android:layout_width="82dp"
            android:layout_height="48dp"
            android:text="출근하기"
            android:background="@drawable/background_btn_primary"
            tools:layout_editor_absoluteX="313dp"
            tools:layout_editor_absoluteY="262dp" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="400dp"
            android:background="#F3F3F3"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
             >

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="280dp"
                android:layout_height="48dp"
                android:background="@drawable/background_round_white"
                tools:layout_editor_absoluteX="65dp"
                tools:layout_editor_absoluteY="253dp">

                <TextView
                    android:id="@+id/textView1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="첫 스탬프"
                    android:textColor="@color/black"
                    android:textSize="14dp"
                    tools:layout_editor_absoluteX="7dp"
                    tools:layout_editor_absoluteY="5dp" />

                <TextView
                    android:id="@+id/textView8"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="/480"
                    android:textColor="@color/black"
                    android:textSize="28dp"
                    android:textStyle="bold"
                    tools:layout_editor_absoluteX="187dp"
                    tools:layout_editor_absoluteY="5dp" />

                <TextView
                    android:id="@+id/textView10"
                    android:layout_width="71dp"
                    android:layout_height="38dp"
                    android:gravity="right"
                    android:text="240"
                    android:textColor="@color/black"
                    android:textSize="28dp"
                    android:textStyle="bold"
                    tools:layout_editor_absoluteX="115dp"
                    tools:layout_editor_absoluteY="5dp" />

                <TextView
                    android:id="@+id/textView9"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="분"
                    android:textColor="@color/black"
                    android:textSize="20dp"
                    tools:layout_editor_absoluteX="245dp"
                    tools:layout_editor_absoluteY="11dp" />

                <TextView
                    android:id="@+id/textView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="08:00"
                    android:textColor="@color/black"
                    android:textSize="14dp"
                    tools:layout_editor_absoluteX="7dp"
                    tools:layout_editor_absoluteY="23dp" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <TextView
                android:id="@+id/textView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="지금까지 일한 시간"
                android:textColor="@color/black"
                android:textSize="16dp"
                android:textStyle="bold"
                tools:layout_editor_absoluteX="65dp"
                tools:layout_editor_absoluteY="224dp" />

            <RadioGroup
                android:id="@+id/radio_Group"
                android:layout_width="280dp"
                android:layout_height="120dp"
                android:background="@drawable/background_round_white"
                android:orientation="vertical"
                app:layout_constraintBaseline_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.496"
                app:layout_constraintStart_toStartOf="parent"
                tools:layout_editor_absoluteY="52dp">


                <RadioButton
                    android:id="@+id/radio_btn1"
                    android:layout_width="wrap_content"
                    android:layout_height="33dp"
                    android:layout_marginTop="6dp"
                    android:background="parent"
                    android:text="회사" />

                <RadioButton
                    android:id="@+id/radio_btn2"
                    android:layout_width="wrap_content"
                    android:layout_height="33dp"
                    android:layout_marginTop="5dp"
                    android:background="parent"
                    android:text="자택/스마트워크센터" />

                <RadioButton
                    android:id="@+id/radio_btn3"
                    android:layout_width="wrap_content"
                    android:layout_height="33dp"
                    android:layout_marginTop="5dp"
                    android:background="parent"
                    android:text="출장" />

            </RadioGroup>

            <TextView
                android:id="@+id/textView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="근무 유형"
                android:textColor="@color/black"
                android:textSize="16dp"
                android:textStyle="bold"
                tools:layout_editor_absoluteX="65dp"
                tools:layout_editor_absoluteY="22dp" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>